        <surfaceGrp xml:id="g.3" n="gathering">
            <surfaceGrp xml:id="f.19" n="folio">
                <surface n="verso">
                    <label>Folio 19 Verso</label>
                    <graphic url="Jesus_Q_G_8_f19v.jpg"/>
                    <zone n="EETS.QD.I">
                        <line n="l.1"><hi rend="touched">Q</hi>uis dabit capiti meo fontem
                                lacr<ex>i</ex>ma<ex>rum</ex><note place="bottom" anchored="true"
                                type="explanatory">The title is in a somewhat sloppy textura rotunda
                                rather than the anglicana of the main body of the
                            text.</note></line>
                    </zone>
                    <zone n="EETS.QD.1">
                        <line n="l.1"><hi rend="capital_missing_3">W</hi>ho shal <hi
                                rend="underline">yeue</hi> vn to <hi rend="underline">myn hede</hi>
                            a well .<note place="bottom" anchored="true" type="explanatory">There is
                                no guide letter, but there is a mark to the left of the space that
                                may indicate the same intent.</note></line>
                        <line n="l.2"><hi rend="touched">O</hi>f bitter terys my sorowes to <hi
                                rend="underline">compleyne</hi> .</line>
                        <line n="l.3"><hi rend="touched">O</hi>r a grete <hi rend="underline"
                                >conduyt</hi> or troubly waters fell .</line>
                        <line n="l.4"><hi rend="touched">D</hi>owne to <hi rend="underline"
                                >distill</hi> frome myne eyne tweyne . </line>
                        <line n="l.5"><hi rend="touched">T</hi>o shew þe <hi rend="underline"
                                >constreynt</hi> of my dedly peyne .</line>
                        <line n="l.6"><hi rend="touched">W</hi>han I allas <hi rend="underline"
                                >behelde</hi> and dide see . </line>
                    </zone>
                </surface>
            </surfaceGrp>
        </surfaceGrp>
        <surfaceGrp xml:id="g.4" n="gathering">
            <surfaceGrp xml:id="f.20" n="folio">
                <surface n="recto">
                    <label>Folio 20 Recto</label>
                    <graphic url="Jesus_Q_G_8_f20r.jpg"/>
                    <zone n="EETS.QD.1">
                        <line n="l.7"><hi rend="touched">M</hi>y dere son <hi rend="underline"
                                >bled</hi><hi rend="underline">e</hi> in eu<ex>er</ex>y weyne . </line>
                        <line n="l.8"><hi rend="touched">B</hi>e twix two <hi rend="underline"
                                >thewes</hi> nayled to a <hi rend="underline">tre</hi> . </line>
                    </zone>
                    <zone n="EETS.QD.2">
                        <line n="l.1"><hi>W</hi>ho shall of <hi rend="underline">wepyng</hi> giff me
                                <hi rend="underline">suffisaunce</hi> .</line>
                        <line n="l.2"><hi rend="touched">O</hi>r to my <hi rend="underline"
                                >sobbyng</hi> who shall my teres fynde . </line>
                        <line n="l.3"><hi rend="touched">T</hi>o see my ioye my hart<ex>is </ex>high
                                <hi rend="underline">plesauncce</hi> . </line>
                        <line n="l.4"><hi rend="touched">M</hi>y swete sone þat <hi rend="underline"
                                >was</hi> to me so kynde .</line>
                        <line n="l.5"><hi rend="underline"><hi rend="touched">R</hi>ailed</hi> with
                            rede bloode as sappe doth þurth <hi rend="underline">þe rynde</hi> . </line>
                        <line n="l.6"><hi rend="touched">T</hi>hurgh his <hi rend="underline"
                                >enmys</hi> wengeable crueltee . </line>
                        <line n="l.7"><hi rend="touched">D</hi>irkenyd with dethe his eyne nowe made
                                <hi rend="underline">blynde</hi><note place="bottom" anchored="true"
                                type="explanatory">The "de" here has been corrected and written over
                                another glyph, but the glyph cannot be made out.</note> .</line>
                        <line n="l.8"><hi rend="touched">A</hi> twix two <hi rend="underline"
                                >thewes</hi> nailed to a <hi rend="underline">tre</hi> .</line>
                    </zone>
                    <zone n="EETS.QD.3">
                        <line n="l.1"><hi>M</hi>y ioye my light my <hi rend="underline">lant</hi><hi
                                rend="underline">er</hi><hi rend="underline">ne </hi>moost entier
                            .</line>
                        <line n="l.2"><hi rend="touched">T</hi>his <hi rend="underline">heuenly</hi>
                                p<hi rend="underline">hebu</hi>s is clepsid of his light .</line>
                        <line n="l.3"><hi rend="touched">Þ</hi>is Esperus hath hidd his bemys <hi
                                rend="underline">clere</hi> . </line>
                        <line n="l.4"><hi rend="touched">A</hi>nd is of now corteynde for my sight
                            .</line>
                        <line n="l.5"><hi rend="touched">W</hi>han shall this day st<ex>er</ex>re
                                <hi rend="underline">shew</hi> me his bemys <hi rend="underline"
                                >bright </hi>.<note place="bottom" anchored="true"
                                type="explanatory">"Sterre" in this line appears to have been
                                inserted into a provided space, as the ink color is darker than the
                                rest of the line and is similar to that of the "de" correction noted
                                above.</note></line>
                        <line n="l.6"><hi rend="touched">T</hi>o clere the <hi rend="underline"
                                >trouble </hi>of myne adu<ex>er</ex>site .</line>
                        <line n="l.7"><hi rend="touched">P</hi><ex>ar</ex>de þe <hi rend="underline"
                                >jewes</hi> doon me to grete <hi rend="underline">vnright</hi>
                            .</line>
                        <line n="l.8"><hi rend="touched">T</hi>o naile my <hi rend="underline"
                                >sone</hi> allas vnto a <hi rend="underline">tre</hi> .</line>
                    </zone>
                    <zone n="EETS.QD.4">
                        <line n="l.1"><hi>A</hi>ll the <hi rend="underline">doughtren </hi>of <hi
                                rend="underline">Ier<ex>usale</ex>m</hi> .<note place="bottom"
                                anchored="true" type="explanatory">the continuous bar beginning over
                                the r, passing through the l, and concluding over the m suggests
                                that the l should be considered as part of an abbreviation rather
                                than as seperate glyph bracketed by two suspensions.</note></line>
                        <line n="l.2"><hi rend="touched">H</hi>aue some compassion on my sighes <hi
                                rend="underline">depe</hi> .</line>
                        <line n="l.3"><hi rend="touched">N</hi>ot like þe <hi rend="underline"
                                >gladnes</hi> which I had in <hi rend="underline">bedlem</hi>
                            .</line>
                        <line n="l.4"><hi rend="touched">C</hi>ome nere of reuthe and <hi
                                rend="underline">helpe</hi> me for to wepe .</line>
                        <line n="l.5"><hi rend="touched">A</hi> swerd of deth doth thorowe my herte
                                <hi rend="underline">crepe</hi> .</line>
                        <line n="l.6"><hi rend="touched">I</hi> fele it <hi rend="underline">full
                                well </hi>of modirly pitee .</line>
                        <line n="l.7"><hi rend="touched">C</hi><hi rend="underline">raumpasshed</hi>
                            with swonyng I do <hi rend="underline">slepee</hi> .</line>
                    </zone>
                </surface>
                <surface n="verso">
                    <label>Folio 20 Verso</label>
                    <graphic url="Jesus_Q_G_8_f20v.jpg"/>
                    <zone n="EETS.QD.4">
                        <line n="l.8"><hi rend="touched">T</hi>o see my son <hi rend="underline"
                                >thus</hi> nailed to <hi rend="underline">a tre</hi> .</line>
                    </zone>
                    <zone n="EETS.QD.5">
                        <line n="l.1"><hi>A</hi> gentill <hi rend="underline">princesses </hi> and
                            ladies of <hi rend="underline">estate</hi> .</line>
                        <line n="l.2"><hi rend="touched">A</hi>nd ye <hi rend="underline"
                                >virgyns</hi> in your<ex>e</ex><ex> </ex>entent moost <hi
                                rend="underline">clene .</hi></line>
                        <line n="l.3"><hi rend="touched">T</hi>o gyff me <hi rend="underline"
                                >comfort </hi>þat stood all desolate .</line>
                        <line n="l.4"><hi rend="touched">R</hi>enneth a paas to se þe <hi
                                rend="underline">woundes</hi> grene .</line>
                        <line n="l.5"><hi rend="touched">O</hi>f your true <hi rend="underline"
                                >spouse </hi>of bleding pale <hi rend="underline">and leue
                            </hi>.</line>
                        <line n="l.6"><hi rend="touched">A</hi>nd <hi rend="underline">adu</hi><hi
                                rend="underline">er</hi><hi rend="underline">tiseth</hi> and hath
                            now reuth on me .</line>
                        <line n="l.7">ffeynt for to stonde for how shuld <hi rend="underline">I
                                sustene</hi> .</line>
                        <line n="l.8"><hi rend="touched">T</hi>o se my <hi rend="underline"
                                >sone</hi> þus <hi rend="underline">nailed</hi> to a<hi
                                rend="underline"> tre .</hi></line>
                    </zone>
                    <zone n="EETS.QD.6">
                        <line n="l.1"><hi>A</hi>nd all ye <hi rend="underline">women</hi> to appeise
                            my <hi rend="underline">heuynes</hi> .</line>
                        <line n="l.2"><hi rend="touched">R</hi>emembrith þe processe of his <hi
                                rend="underline">dredfull</hi> victory .</line>
                        <line n="l.3"><hi rend="touched">S</hi>e to fore pilat by <hi
                                rend="underline">many</hi> false witnes .</line>
                        <line n="l.4"><hi rend="touched">H</hi>ow he was <hi rend="underline"
                                >dampned</hi> in þe consistorye .</line>
                        <line n="l.5"><hi rend="touched">R</hi>edd ye euer or saw in <hi
                                rend="underline">historye</hi> .</line>
                        <line n="l.6"><hi rend="touched">O</hi>f ony <hi rend="underline"
                                >sorwes</hi> þat may compared be .</line>
                        <line n="l.7"><hi rend="touched">V</hi>nto the sorwes <hi rend="underline"
                                >graue </hi>in my <hi rend="underline">memorie</hi> . </line>
                        <line n="l.8"><hi rend="touched">T</hi>o se my sone þus <hi rend="underline"
                                >nailed</hi> to a <hi rend="underline">tre</hi> .</line>
                    </zone>
                    <zone n="EETS.QD.7">
                        <line n="l.1"><hi>A</hi>nd if ȝe like of <hi rend="underline"
                                >reuth</hi><ex> </ex>for to lere .</line>
                        <line n="l.2"><hi rend="touched">A</hi>nd at my <hi rend="underline"
                                >terys</hi> yeue ȝe not <hi rend="underline">disdeyne</hi> .</line>
                        <line n="l.3"><hi rend="touched">B</hi>ot of <hi rend="underline"
                                >compassion</hi> mekely list to here .</line>
                        <line n="l.4"><hi rend="touched">H</hi>ow a sharpe swerde my <hi
                                rend="underline">herte</hi> hath carue on <hi rend="underline"
                                >tweyne</hi> .</line>
                        <line n="l.5"><hi rend="touched">A</hi> swerde of sorowe <hi
                                rend="underline">þurth</hi> p<ex>er</ex>cide eu<ex>er</ex>y vayne
                            .</line>
                        <line n="l.6"><hi rend="touched">H</hi>ow deth hath <hi rend="underline"
                                >slayne</hi> my sone and <hi rend="underline">sparith me</hi>
                            .</line>
                        <line n="l.7"><hi rend="underline"><hi rend="touched">A</hi>llas</hi> fro
                            wepyng how sholde I me restreyne .</line>
                        <line n="l.8"><hi rend="touched">T</hi>o se my <hi rend="underline"
                                >sone</hi> þus <hi rend="underline">nailed</hi> to a <hi
                                rend="underline">tre</hi> .</line>
                    </zone>
                </surface>
            </surfaceGrp>
            <surfaceGrp xml:id="f.21" n="folio">
                <surface n="recto">
                    <label>Folio 21 Recto</label>
                    <graphic url="Jesus_Q_G_8_f21r.jpg"/>
                    <zone n="EETS.QD.8">
                        <line n="l.1"><hi>P</hi>peple <hi rend="underline">vnkynde</hi> why wist
                                þ<ex>u</ex> non<ex>e </ex>hede <hi rend="underline">take</hi>
                            .</line>
                        <line n="l.2"><hi rend="touched">T</hi>o se thy lord of hell erth and <hi
                                rend="underline">heuen</hi> .</line>
                        <line n="l.3"><hi rend="touched">M</hi>eeke as a <hi rend="underline"
                                >lambe</hi> thus offrid for þi sake .</line>
                        <line n="l.4"><hi rend="touched">T</hi>o sle þe dragon with his <hi
                                rend="underline">hed</hi><hi rend="underline">is</hi><hi
                                rend="underline"> seuen .</hi></line>
                        <line n="l.5"><hi rend="underline"><hi rend="touched">D</hi>aunting</hi> þe
                            power of his infernal leuen<ex>e </ex>.</line>
                        <line n="l.6"><hi rend="touched">O</hi>ut of his thraldome to <hi
                                rend="underline">make</hi> ye go fre .</line>
                        <line n="l.7"><hi rend="touched">W</hi><ex>ith</ex> many mo <hi
                                rend="underline">woundes</hi> þan eny man can <hi rend="underline"
                                >neven</hi><hi rend="underline">e</hi><hi rend="underline"> .
                            </hi></line>
                        <line n="l.8"><hi rend="touched">W</hi>han he at calvarie was nailed to a
                                <hi rend="underline">tre</hi> .</line>
                    </zone>
                    <zone n="EETS.QD.9">
                        <line n="l.1"><hi>I</hi>t is a m<ex>er</ex>vale or any maner <hi
                                rend="underline">wounder . </hi></line>
                        <line n="l.2"><hi rend="touched">Þ</hi>of I full oft <hi rend="underline"
                                >wowne</hi> for greuaunce .</line>
                        <line n="l.3"><hi rend="touched">W</hi>as <hi rend="underline">euer</hi>
                            moder outhir here or yonder .</line>
                        <line n="l.4"><hi rend="touched">T</hi>hat for hire childe felt more <hi
                                rend="underline">Penaunce .</hi></line>
